    <!-- jQuery -->
    <script src="~/Areas/AdminPanel/AdminFront/assets/js/jquery-3.5.0.min.js"></script>
    <!-- Bootstrap Core JS -->
    <script src="~/Areas/AdminPanel/AdminFront/assets/js/popper.min.js"></script>
    <script src="~/Areas/AdminPanel/AdminFront/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- Slimscroll JS -->
    <script src="~/Areas/AdminPanel/AdminFront/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <!-- Custom JS -->
    <script src="~/Areas/AdminPanel/AdminFront/assets/js/admin.js"></script>
</body>
</html>