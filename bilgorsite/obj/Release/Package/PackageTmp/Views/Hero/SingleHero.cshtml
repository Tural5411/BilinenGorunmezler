@model bilgorsite.ViewModel.VideoVM
<div class="bp-element bp-element-st-list-videos vblog-layout-1">
    <div class="wrap-element">
        <div class="feature-item">
            <div class="row">
                @foreach (var x in Model.videoList)
                {
                    <div class="col-lg-9">
                        <div class="video">
                            <img src="@x.tbl_photo.URL" alt="@x.Title">
                            <div class="overlay"></div>
                            <div class="meta-info">
                                <div class="imdb">
                                    <span class="value">5</span>IMDb
                                </div>
                                <div class="label">
                                    HDRip
                                </div>
                            </div>
                            <a href="https://www.youtube.com/watch?v=hNQFjqDvPhA&amp;feature=youtu.be" class="btn-play popup-youtube">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="text">
                            <h4 class="title">
                                <a href="single-video.html">
                                    @x.Title
                                </a>
                            </h4>
                            <a href="single-video.html" class="btn-readmore btn-normal shape-round">
                                Etrafli
                            </a>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
</div>
